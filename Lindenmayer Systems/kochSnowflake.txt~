# Image positioning and dimensions
Dimensions : 1920, 1080
Position : -300, -300

# Number of iterations
Iterations : 5

# Default angle
Angle : '60

# Default line length
Linelength : 3

# Default line width
Linewidth : 1

# Default line color
Linecolor : #00ff00

# Default background color
Background : #ffffff

# Axiom definition
Axiom : F++F++F 

# Definition of mappings
F = cF-F++F-F
